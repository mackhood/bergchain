object vonLukaz
{
 var energia = 150
 var diversion = 70
 var colorRopa = "blanca"
 var edad = 17
  method devolverRopa(){
  return ropa
 }
  method devolverEdad(){
  return edad
  }
  
  method modificarEnergia(cantidad){
  energia -=cantidad
  }
  
  method modificarDiversion (cantidad) {
  diversion -=cantidad
  }
  
  
 
}

